  font-size: 20px;
  border: 1px solid;
  border-radius: 50%;
  text-indent: 0;
  text-align: center;
}
.app .goods-list .inner-content {
  display: inline-block;
  vertical-align: middle;
  height: auto;
  position: relative;
  width: 100%;
  text-decoration: inherit;
  border-radius: inherit;
  z-index: 1;
}

.map .map-module {
  width: 100%;
  height: calc(100% - 30px);
